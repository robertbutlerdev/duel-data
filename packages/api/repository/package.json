{
  "name": "@repo/api-repository",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "author": {
    "name": "Robert Butler",
    "email": "me@robertbutler.dev"
  },
  "exports": {
    "./*": {
      "types": "./src/*.ts",
      "default": "./dist/*.js"
    },
    "./utils/*": {
      "types": "./src/utils/*.ts",
      "default": "./dist/utils/*.js"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint 'src'",
    "lint-fix": "eslint 'src' --fix"
  },
  "dependencies": {
    "@repo/api-common": "workspace:*",
    "@repo/api-entity": "workspace:*",
    "@tediousjs/connection-string": "^0.6.0",
    "kysely": "^0.28.0",
    "tarn": "^3.0.2",
    "tedious": "^18.6.1"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/tsconfig": "workspace:*",
    "eslint": "9.28.0",
    "tslib": "2.8.1",
    "typescript": "5.8.3"
  }
}